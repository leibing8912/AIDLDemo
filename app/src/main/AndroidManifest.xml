<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.jianke.aidldemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".module.activity.ClientActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 用户信息管理服务 -->
        <service android:name=".module.service.UserInfoManagerService" >
            <intent-filter>
                <action android:name="cn.jianke.aidldemo.module.service.UserInfoManagerService" />
            </intent-filter>
        </service>
    </application>
</manifest>